{
  "name": "demo-dapp",
  "description": "Ethereum Demo Dapp built on Sequence stack",
  "version": "0.1.0",
  "private": true,
  "homepage": "demo-dapp",
  "scripts": {
    "dev": "BROWSER=none pnpm start",
    "start": "vite",
    "build": "BUILD_PATH='./dist' tsc && vite build",
    "serve": "vite preview",
    "dist": "pnpm build",
    "link-sequence": "pnpm run clear:vite:cache && ../sequence.js/scripts/pnpm-link.sh link",
    "unlink-sequence": "pnpm run clear:vite:cache && ../sequence.js/scripts/pnpm-link.sh unlink",
    "clear:vite:cache": "rm -rf node_modules/.vite/"
  },
  "dependencies": {
    "0xsequence": "^1.6.2",
    "@0xsequence/design-system": "^1.0.14",
    "@0xsequence/ethauth": "^0.8.1",
    "@0xsequence/network": "^1.6.2",
    "@0xsequence/provider": "^1.6.2",
    "@0xsequence/utils": "^1.6.2",
    "@types/node": "^16.11.22",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@vanilla-extract/css": "^1.9.3",
    "ethers": "^5.7.2",
    "framer-motion": "^9.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^4.5.5"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@vanilla-extract/vite-plugin": "^3.7.1",
    "@vitejs/plugin-react": "^3.1.0",
    "vite": "^4.1.1",
    "vite-plugin-svgr": "^2.4.0",
    "vite-tsconfig-paths": "^4.0.5"
  }
}
