{
  "name": "demo-dapp",
  "version": "0.1.0",
  "description": "Ethereum Demo Dapp built on Sequence stack",
  "private": true,
  "license": "none",
  "scripts": {
    "start": "yarn dev",
    "dev": "webpack-dev-server --mode development --progress --color --config config/webpack/webpack.config.local.js",
    "dist": "yarn dist:clean && yarn dist:build",
    "dist:build": "webpack --mode production --color --config config/webpack/webpack.config.dist.js",
    "dist:clean": "rimraf ./dist",
    "dist:stats": "serve -l 5555 ./dist/.stats",
    "lint": "tslint -c tslint.json 'src/**/**.ts{,x}'",
    "lint:fix": "tslint -c tslint.json --fix 'src/**/*.ts{,x}'",
    "test:watch": "jest --watchAll",
    "test": "jest --ci --runInBand",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "link-sequence": "yarn link 0xsequence @0xsequence/abi @0xsequence/api @0xsequence/auth @0xsequence/config @0xsequence/guard @0xsequence/multicall @0xsequence/network @0xsequence/provider @0xsequence/relayer @0xsequence/transactions @0xsequence/utils @0xsequence/wallet",
    "unlink-sequence": "yarn unlink 0xsequence @0xsequence/abi @0xsequence/api @0xsequence/auth @0xsequence/config @0xsequence/guard @0xsequence/multicall @0xsequence/network @0xsequence/provider @0xsequence/relayer @0xsequence/transactions @0xsequence/utils @0xsequence/wallet"
  },
  "dependencies": {
    "0xsequence": "^0.35.10",
    "@0xsequence/ethauth": "0.6.0",
    "core-js": "^3.6.1",
    "emotion": "^10.0.27",
    "ethers": "^5.5.1",
    "global": "^4.3.2",
    "history": "^4.10.1",
    "micro-observables": "^1.3.3",
    "polished": "^3.6.3",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "^6.0.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "theme-ui": "^0.3.1",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@hot-loader/react-dom": "^16.13.0",
    "@types/classnames": "^2.2.9",
    "@types/node": "^14.0.5",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-router": "^5.1.7",
    "@types/react-router-dom": "^5.1.5",
    "@types/react-window": "^1.8.1",
    "@types/theme-ui": "^0.3.1",
    "@types/webpack-env": "^1.15.2",
    "copy-webpack-plugin": "^6.0.1",
    "css-loader": "^3.5.3",
    "dotenv": "8.2.0",
    "express": "^4.16.4",
    "fork-ts-checker-webpack-plugin": "^4.1.5",
    "html-webpack-plugin": "^4.3.0",
    "prettier": "^2.0.5",
    "react-hot-loader": "^4.12.21",
    "rimraf": "^3.0.2",
    "serve": "^11.3.1",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^3.0.1",
    "ts-loader": "^7.0.5",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "^6.1.2",
    "tslint-react": "^5.0.0",
    "typescript": "^3.9.3",
    "typescript-styled-plugin": "^0.15.0",
    "url-loader": "^4.1.0",
    "webpack": "^4.41.5",
    "webpack-assets-by-type-plugin": "^0.1.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.11.0"
  }
}
